{
  "apiVersion": "1.0.0",
  "swaggerVersion": "1.2",
  "basePath1": "http://localhost:3000",
  "basePath": "https://vehicle-history.io",
  "resourcePath": "/api/vehicle-history",
  "apis": [
    {
      "path": "/api/vehicle-history",

      "operations": [
        {
          "produces": [
            "application/vnd.vehicle-history.v1+json"
          ],
          "consumes": [
            "application/json"
          ],
          "method": "GET",
          "summary": "Find vehicle history",
          "notes": "Returns a vehicle info and history",
          "type": "Response",
          "nickname": "postVehicleHistory",
          "authorizations": {},
          "parameters": [
            {
              "name": "plate",
              "description": "Vehicle plate number",
              "required": true,
              "type": "string",
              "defaultValue": "BBC12345",
              "paramType": "query",
              "allowMultiple": false
            },
            {
              "name": "vin",
              "description": "Vehicle vin number",
              "required": false,
              "type": "string",
              "defaultValue": "ABC123456789DEF",
              "paramType": "query",
              "allowMultiple": false
            },
            {
              "name": "firstRegistrationDate",
              "description": "Vehicle first registration date",
              "required": false,
              "type": "string",
              "defaultValue": "11.12.2014",
              "paramType": "query",
              "allowMultiple": false
            },
            {
              "name": "country",
              "description": "Vehicle registration country",
              "required": false,
              "type": "string",
              "defaultValue": "PL",
              "paramType": "query",
              "allowMultiple": false
            }
          ],
          "responseMessages": [
            {
              "code": 404,
              "message": "Unable to find vehicle history"
            }
          ]
        }
      ]
    }
  ],
